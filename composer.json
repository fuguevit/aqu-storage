{
  "name": "fuguevit/storage",
  "type": "library",
  "description": "Extend laravel filesystem with aliyun, qiniu, and ucloud",
  "license": "MIT",
  "homepage": "http://www.fuguevit.com",
  "keywords": ["filesystem","laravel","aliyun","qiniu","ucloud"],
  "authors": [
    {
      "name": "Frank Wang",
      "email": "wangxuan@188.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/config": "~5.2",
    "illuminate/support": "~5.2",
    "illuminate/filesystem": "~5.2",
    "aliyuncs/oss-sdk-php": "~2.0",
    "qiniu/php-sdk": "7.*",
    "vlucas/phpdotenv": "^2.4",
    "upyun/sdk": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "orchestra/testbench": "3.3.x-dev",
    "orchestra/database": "3.3.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Fuguevit\\Storage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fuguevit\\Storage\\Tests\\": "tests/"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
